{
  "name": "fractal/swisseph-php",
  "description": "Pure PHP 8.1+ port of Swiss Ephemeris (approximate)",
  "type": "library",
  "require": {
    "php": ">=8.1"
  },
  "autoload": {
    "psr-4": {
      "Swisseph\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Swisseph\\Tests\\": "tests/phpunit/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "^10.5 || ^11.0",
    "squizlabs/php_codesniffer": "^3.9"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "lint": "vendor/bin/phpcs -q --report=full",
    "lint:fix": "vendor/bin/phpcbf -q"
  }
}
